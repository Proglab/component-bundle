{
  "name": "proglab/component-bundle",
  "version": "1.0.0",
  "type": "symfony-bundle",
  "description": "Components bundle for Symfony",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "symfony/asset": "7.1.*",
    "symfony/asset-mapper": "7.1.*",
    "symfony/stimulus-bundle": "^2.17",
    "symfony/framework-bundle": "7.1.*",
    "symfony/translation": "7.1.*",
    "symfony/twig-bundle": "7.1.*",
    "symfony/ux-live-component": "^2.17",
    "symfony/ux-twig-component": "^2.17",
    "symfony/yaml": "7.1.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Proglab\\ComponentBundle\\": "src/"
